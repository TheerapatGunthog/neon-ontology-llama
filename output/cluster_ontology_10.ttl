@prefix :    <http://example.org/list_subject#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

: a owl:Ontology ;
  rdfs:label "list_subject Ontology"@en ;
  rdfs:comment "Ontology induced from list_subject titles clustered by embeddings."@en .

:Subject a owl:Class ;
  rdfs:label "Subject"@en ;
  rdfs:comment "A subject or course item."@en .

:subject_name_en a owl:DatatypeProperty , owl:FunctionalProperty ;
  rdfs:label "Subject Name (English)"@en ;
  rdfs:domain :Subject ;
  rdfs:range xsd:string .

:subject_name_en_edit a owl:DatatypeProperty ;
  rdfs:label "Subject Name (English, Edited)"@en ;
  rdfs:domain :Subject ;
  rdfs:range xsd:string .

:LanguageCommunication a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "LanguageCommunication"@en ;
  rdfs:comment "Courses focused on conversational skills in various languages."@en .

:HealthAndWellness a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "HealthAndWellness"@en ;
  rdfs:comment "Courses aimed at promoting health and well-being through various practices."@en .

:BusinessAndEconomics a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "BusinessAndEconomics"@en ;
  rdfs:comment "Courses covering topics in business management, economics, and technology."@en .

:HealthAndWellness skos:related :LanguageCommunication .
:BusinessAndEconomics skos:related :HealthAndWellness .
:BusinessAndEconomics skos:related :LanguageCommunication .

