: a owl:Ontology ;
  rdfs:label "separate_files_dataset Ontology"@en ;
  rdfs:comment "Multi-column ontology induced via LLM-driven NeOn conceptualization and population. Rows are independent unless LINK_BY_ROW enabled."@en .

:Record a owl:Class ; rdfs:label "Record"@en .

:hasSubject a owl:ObjectProperty ;
  rdfs:domain :Record ;
  rdfs:range  :SubjectNameEn ;
  rdfs:label "hasSubject"@en .

:hasJobTitle a owl:ObjectProperty ;
  rdfs:domain :Record ;
  rdfs:range  :JobTitle ;
  rdfs:label "hasJobTitle"@en .

:hasJobType a owl:ObjectProperty ;
  rdfs:domain :Record ;
  rdfs:range  :JobType ;
  rdfs:label "hasJobType"@en .

:hasSkill a owl:ObjectProperty ;
  rdfs:domain :Record ;
  rdfs:range  :Skill ;
  rdfs:label "hasSkill"@en .

:hasJobType a owl:ObjectProperty ;
  rdfs:domain :JobTitle ;
  rdfs:range  :JobType ;
  rdfs:label "hasJobType"@en .

:SubjectNameEn a owl:Class ;
  rdfs:label "SubjectNameEn"@en ;
  rdfs:comment "Top concept for column 'subject_name_en' with role 'Subject name'."@en .

:JobTitle a owl:Class ;
  rdfs:label "JobTitle"@en ;
  rdfs:comment "Top concept for column 'Job_Title' with role 'Job title'."@en .

:JobType a owl:Class ;
  rdfs:label "JobType"@en ;
  rdfs:comment "Top concept for column 'Job_Type' with role 'Job type'."@en .

:Skill a owl:Class ;
  rdfs:label "Skill"@en ;
  rdfs:comment "Top concept for column 'Skill' with role 'Skill list'."@en .

:Languagecommunication a owl:Class ;
