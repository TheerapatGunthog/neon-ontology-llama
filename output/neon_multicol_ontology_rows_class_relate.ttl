  rdfs:label "Languagecommunication"@en ;
  rdfs:comment "Courses teaching language skills for effective communication."@en .

:Healthexercise a owl:Class ;
  rdfs:subClassOf :SubjectNameEn ;
  rdfs:label "Healthexercise"@en ;
  rdfs:comment "Programs focused on physical exercise to improve health."@en .

:Economics a owl:Class ;
  rdfs:subClassOf :SubjectNameEn ;
  rdfs:label "Economics"@en ;
  rdfs:comment "Studies of economic theories, principles, and applications."@en .

:Innovationtechnology a owl:Class ;
  rdfs:subClassOf :SubjectNameEn ;
  rdfs:label "Innovationtechnology"@en ;
  rdfs:comment "Exploration of new technologies and innovative practices."@en .

:Businessmanagement a owl:Class ;
  rdfs:subClassOf :SubjectNameEn ;
  rdfs:label "Businessmanagement"@en ;
  rdfs:comment "Management principles for small businesses and entrepreneurship."@en .

:Accounting a owl:Class ;
  rdfs:subClassOf :SubjectNameEn ;
  rdfs:label "Accounting"@en ;
  rdfs:comment "Fundamental accounting concepts and practices."@en .

:Informationwriting a owl:Class ;
  rdfs:subClassOf :SubjectNameEn ;
  rdfs:label "Informationwriting"@en ;
  rdfs:comment "Techniques for gathering and presenting information in reports."@en .

:Computeroffice a owl:Class ;
  rdfs:subClassOf :SubjectNameEn ;
  rdfs:label "Computeroffice"@en ;
  rdfs:comment "Computer applications used in office productivity."@en .

:Businessmanagement skos:broader :Accounting .
