@prefix :    <http://example.org/list_subject#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

: a owl:Ontology ;
  rdfs:label "list_subject Ontology"@en ;
  rdfs:comment "Ontology induced from list_subject via LLM-driven NeOn conceptualization and population."@en .

:Subject a owl:Class ;
  rdfs:label "Subject"@en ;
  rdfs:comment "Top concept for course titles or subjects."@en .

:subject_name_en a owl:DatatypeProperty , owl:FunctionalProperty ;
  rdfs:label "Subject Name (English)"@en ;
  rdfs:domain :Subject ;
  rdfs:range xsd:string .

:subject_name_en_edit a owl:DatatypeProperty ;
  rdfs:label "Subject Name (English, Edited)"@en ;
  rdfs:domain :Subject ;
  rdfs:range xsd:string .

:Japaneseconversation a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Japaneseconversation"@en ;
  rdfs:comment "Basic conversational skills in Japanese for everyday interactions."@en .

:Koreancommunication a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Koreancommunication"@en ;
  rdfs:comment "Fundamental Korean language skills for effective communication."@en .

:Languagecommunication a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Languagecommunication"@en ;
  rdfs:comment "Study of language skills used for interpersonal communication."@en .

:Healthexercise a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Healthexercise"@en ;
  rdfs:comment "Physical activities aimed at improving health and fitness."@en .

:Economysufficiency a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Economysufficiency"@en ;
  rdfs:comment "Principles of sufficiency economy and sustainable living."@en .

:Innovationtechnology a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Innovationtechnology"@en ;
  rdfs:comment "Emerging technologies and innovative practices shaping modern society."@en .

:Smallbusinessentrepreneurship a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Smallbusinessentrepreneurship"@en ;
  rdfs:comment "Management strategies for small business entrepreneurs."@en .

:Microeconomics a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Microeconomics"@en ;
  rdfs:comment "Analysis of individual economic agents and market behavior."@en .

:Reportwritinginformation a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Reportwritinginformation"@en ;
  rdfs:comment "Techniques for gathering and presenting information in reports."@en .

:Accountingbasics a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Accountingbasics"@en ;
  rdfs:comment "Fundamental accounting concepts and financial reporting."@en .

:Computerofficeapplication a owl:Class ;
  rdfs:subClassOf :Subject ;
  rdfs:label "Computerofficeapplication"@en ;
  rdfs:comment "Use of computer applications in office productivity."@en .

:Languagecommunication skos:broader :Japaneseconversation .
:Languagecommunication skos:broader :Koreancommunication .
:Smallbusinessentrepreneurship skos:related :Accountingbasics .
:Microeconomics skos:related :Economysufficiency .
:Computerofficeapplication skos:related :Innovationtechnology .
:Reportwritinginformation skos:related :Accountingbasics .

# ---- instances ----
:FundamentalJapaneseConversation a :Japaneseconversation ;
  :subject_name_en "Fundamental Japanese Conversation"@en .
:KoreanForCommunication a :Koreancommunication ;
  :subject_name_en "Korean for Communication"@en .
:ExerciseForHealth a :Healthexercise ;
  :subject_name_en "Exercise for Health"@en .
:SufficiencyEconomyAndWisdomOfLiving a :Economysufficiency ;
  :subject_name_en "Sufficiency Economy and Wisdom of Living"@en .
:InnovationAndTechnology a :Innovationtechnology ;
  :subject_name_en "Innovation and Technology"@en .
:EntrepreneurSmallBusinessManagement a :Smallbusinessentrepreneurship ;
  :subject_name_en "Entrepreneur Small Business Management"@en .
:Microeconomics a :Microeconomics ;
  :subject_name_en "Microeconomics"@en .
:InformationForReportWriting a :Reportwritinginformation ;
  :subject_name_en "Information for Report Writing"@en .
:IntroductionToAccounting a :Accountingbasics ;
  :subject_name_en "Introduction to Accounting"@en .
:ComputerApplicationInOffice a :Computerofficeapplication ;
  :subject_name_en "Computer Application in Office"@en .
