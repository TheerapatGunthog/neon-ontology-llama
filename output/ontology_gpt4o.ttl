@prefix : <http://example.org/list_subject> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ListSubject a owl:Ontology ;
    rdfs:label "List Subject Ontology" ;
    rdfs:comment "An ontology for subjects in a list." .

:Subject a owl:Class ;
    rdfs:label "Subject" ;
    rdfs:comment "A class representing a subject." .

:subject_name_en a owl:DatatypeProperty ;
    rdfs:label "Subject Name (English)" ;
    rdfs:comment "The name of the subject in English." ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string .

:subject_name_en_edit a owl:DatatypeProperty ;
    rdfs:label "Subject Name (English Edit)" ;
    rdfs:comment "The edited name of the subject in English." ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string .

